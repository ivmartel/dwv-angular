{
  "name": "dwv-angular",
  "version": "0.11.0-beta.0",
  "description": "Medical viewer using DWV (DICOM Web Viewer) and Angular.",
  "license": "GPL-3.0",
  "author": "ivmartel",
  "homepage": "https://ivmartel.github.io/dwv-angular/",
  "repository": {
    "type": "git",
    "url": "git://github.com/ivmartel/dwv-angular.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "ng build dwv-angular --configuration production",
    "build-app-trunk": "ng build dwv-angular-app --configuration production --base-href /dwv-angular/demo/trunk/",
    "build-app-stable": "ng build dwv-angular-app --configuration production --base-href /dwv-angular/demo/stable/",
    "watch-lib": "ng build dwv-angular --watch --configuration development",
    "test": "ng test",
    "test-ci": "ng test --coverage --watch false --progress false",
    "lint": "ng lint",
    "clean": "ng cache clean"
  },
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.5",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.5",
    "@angular/material": "^21.0.3",
    "@angular/platform-browser": "^21.0.5",
    "@angular/platform-browser-dynamic": "^21.0.5",
    "@angular/service-worker": "^21.0.6",
    "add": "2.0.6",
    "dwv": "^0.35.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.5",
    "@types/jsdom": "^27",
    "@vitest/coverage-v8": "^4.0.16",
    "angular-eslint": "21.1.0",
    "canvas": "^3.2.0",
    "eslint": "^9.39.2",
    "jsdom": "^27.3.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.16"
  },
  "packageManager": "yarn@4.12.0"
}
