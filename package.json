{
  "name": "dwv-angular",
  "version": "0.11.0-beta.0",
  "description": "Medical viewer using DWV (DICOM Web Viewer) and Angular.",
  "license": "GPL-3.0",
  "author": "ivmartel",
  "homepage": "https://ivmartel.github.io/dwv-angular/",
  "repository": {
    "type": "git",
    "url": "git://github.com/ivmartel/dwv-angular.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "ng build dwv-angular --configuration production",
    "build-app-trunk": "ng build dwv-angular-app --configuration production --base-href /dwv-angular/demo/trunk/",
    "build-app-stable": "ng build dwv-angular-app --configuration production --base-href /dwv-angular/demo/stable/",
    "watch-lib": "ng build dwv-angular --watch --configuration development",
    "test": "ng test",
    "test-hl": "ng test --browsers ChromeHeadless",
    "test-ci": "ng test --browsers ChromeHeadless --code-coverage --watch false --progress false",
    "lint": "ng lint",
    "clean": "ng cache clean"
  },
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^20.2.0",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^20.2.0",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/service-worker": "^21.0.3",
    "add": "2.0.6",
    "dwv": "^0.35.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@types/jasmine": "~5.1.12",
    "angular-eslint": "21.1.0",
    "eslint": "^9.37.0",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "yarn@4.12.0"
}
