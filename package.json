    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/material": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "@angular/service-worker": "^18.0.2",
    "dwv": "0.33.1",
    "rxjs": "^7.8.1",
    "stream": "^0.0.3",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.2",
