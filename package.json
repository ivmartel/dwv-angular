{
  "name": "dwv-angular",
  "version": "0.9.1",
  "description": "Medical viewer using DWV (DICOM Web Viewer) and Angular.",
  "license": "GPL-3.0",
  "author": "ivmartel",
  "homepage": "https://ivmartel.github.io/dwv-angular/",
  "repository": {
    "type": "git",
    "url": "git://github.com/ivmartel/dwv-angular.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "ng build dwv-angular --configuration production",
    "build-app-trunk": "ng build dwv-angular-app --configuration production --base-href /dwv-angular/demo/trunk/",
    "build-app-stable": "ng build dwv-angular-app --configuration production --base-href /dwv-angular/demo/stable/",
    "watch-lib": "ng build dwv-angular --watch --configuration development",
    "test": "ng test",
    "test-hl": "ng test --browsers ChromeHeadless",
    "test-ci": "ng test --browsers ChromeHeadless --code-coverage --watch false --progress false",
    "lint": "ng lint",
    "clean": "ng cache clean"
  },
  "dependencies": {
    "@angular/animations": "^19.0.1",
    "@angular/cdk": "^19.0.1",
    "@angular/common": "^19.0.1",
    "@angular/compiler": "^19.0.1",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.1",
    "@angular/material": "^19.0.1",
    "@angular/platform-browser": "^19.0.1",
    "@angular/platform-browser-dynamic": "^19.0.1",
    "@angular/service-worker": "^19.0.1",
    "add": "2.0.6",
    "dwv": "^0.34.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.1",
    "@types/jasmine": "~5.1.5",
    "angular-eslint": "19.0.1",
    "eslint": "^9.16.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.0.1",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.16.0"
  }
}